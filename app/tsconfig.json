{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noEmit": true,
    "allowJs": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["nativewind/types"]
  },
  "include": ["App.tsx", "src/**/*"],
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
